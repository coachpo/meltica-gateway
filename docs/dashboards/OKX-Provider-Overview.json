      "id": 105,
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(meltica_eventbus_publish_duration_bucket{environment=\"$environment\",provider=\"$provider\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "title": "Eventbus Publish Latency (OKX)",
      "type": "timeseries"
    },
    {
