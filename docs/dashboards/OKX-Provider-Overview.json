          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(adapter_extension_events{environment=\"$environment\",provider=\"$provider\"}[1m]))",
          "legendFormat": "events/sec",
          "refId": "A"
        }
      ],
      "title": "Extension Volume by Provider",
      "type": "timeseries"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Distribution of publish latency for OKX-scoped events",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never",
            "axisLabel": "ms"
          },
          "unit": "ms"
        }
      },
