  "links": [],
  "panels": [
    {
      "datasource": {
        "uid": "$datasource"
      },
      "gridPos": {
        "h": 2,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 100,
      "options": {
        "content": "# OKX Provider Overview\n\nExtension throughput and canonical event metrics filtered to the OKX adapter",
        "mode": "markdown"
      },
      "type": "text"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Canonical event throughput produced by OKX",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never",
            "axisLabel": "Events/sec"
          },
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 2
      },
      "id": 101,
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(meltica_eventbus_events_published{environment=\"$environment\",provider=\"$provider\"}[5m])) by (event_type)",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ],
      "title": "Event Publish Rate by Type (OKX)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Custom extension payload rate per symbol for OKX",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never",
            "axisLabel": "Events/sec"
          },
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 2
      },
      "id": 102,
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(adapter_extension_events{environment=\"$environment\",provider=\"$provider\"}[5m])) by (symbol)",
          "legendFormat": "{{symbol}}",
          "refId": "A"
        }
      ],
      "title": "Extension Events per Symbol",
      "type": "timeseries"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Rolling average payload size derived from adapter_extension_payload_bytes_*",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never",
            "axisLabel": "Bytes"
          },
          "unit": "bytes"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "id": 103,
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum by (symbol)(rate(adapter_extension_payload_bytes_sum{environment=\"$environment\",provider=\"$provider\"}[5m])) / clamp_min(sum by (symbol)(rate(adapter_extension_payload_bytes_count{environment=\"$environment\",provider=\"$provider\"}[5m])), 1)",
          "legendFormat": "{{symbol}}",
          "refId": "A"
        }
      ],
      "title": "Average Extension Payload Size",
      "type": "timeseries"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "description": "Aggregate extension throughput (sanity check)",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never",
            "axisLabel": "Events/sec"
          },
          "unit": "ops"
